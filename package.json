		"url": "https://github.com/alharirihamzah/Smart-mirror/issues"
	},
	"homepage": "https://github.com/alharirihamzah/Smart-Mirror/blob/master/README.md",
	"main": "index.js",
	"scripts": {
		"dev": "webpack --config webpack.development.config.js --progress --watch",
		"server": "NODE_ENV=development nodemon server/index.js",
		"build:dev": "webpack --config webpack.development.config.js --colors",
		"build:stage": "webpack --config webpack.production.config.js --progress --colors",
		"build:prod": "webpack --config webpack.production.config.js --progress --colors",
		"start": "concurrently \"yarn server\" \"yarn dev\""
	},
	"dependencies": {
		"express": "^4.16.3",
		"prop-types": "^15.5.7",
		"react": "16.4.2",
		"react-dom": "16.4.2"
	},
